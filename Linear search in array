#include <stdio.h>

int main() {
    int n, i, key, found = 0;
    int arr[100];

    // Read size of array
    scanf("%d", &n);

    // Read array elements
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Read search element
    scanf("%d", &key);

    // Linear search
    for (i = 0; i < n; i++) {
        if (arr[i] == key) {
            printf("found at position %d\n", i);
            found = 1;
            break;
        }
    }

    // If element not found
    if (!found) {
        printf("%d not found\n", key);
    }

    return 0;
}


